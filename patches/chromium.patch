+  arguments->GetRemaining(&v8_args);
+  mojom::EventFilteringInfoPtr info = mojom::EventFilteringInfo::New();
+  info->instance_id = filter->Get(context, gin::StringToSymbol(isolate, "instanceId")).ToLocalChecked().As<v8::Int32>()->Value();
