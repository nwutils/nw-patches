+      function workerAssert(condition, message) {
+        if (!condition) postMessage(`Check failed: ${message}`);
+      }
+
+      function workerAssertEquals(expected, actual, message) {
+        if (expected != actual)
+          postMessage(`Check failed (${message}): ${expected} != ${actual}`);
+      }
+
+      const kOffset1 = 47n;
+      const kOffset2 = 128n;
