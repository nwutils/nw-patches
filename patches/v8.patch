+      }
+      if (call_mode_ == kFromRegisters) {
+        return CallArguments(new_receiver_mode, reg_count_ - 1, regs_[1],
+                             regs_[2]);
+      }
+      return CallArguments(new_receiver_mode, reglist_.PopLeft());
+    }
